{
  "name": "ngpack",
  "version": "1.0.0",
  "description": "AngularJS+Webpack workflow",
  "main": "index.js",
  "scripts": {
    "test": "webpack",
    "clean": "rm -rf build",
    "devmode": "export NODE_ENV=development",
    "dev": "npm run devmode && webpack-dev-server --content-base src/ --hot --history-api-fallback --inline --progress",
    "build": "npm run clean && export NODE_ENV=production && webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/catindev/ngpack.git"
  },
  "author": "catindev",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "jshint-loader": "^0.8.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "angular-component": "github:toddmotto/angular-component",
    "angular-route": "^1.4.8",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-stage-0": "^6.3.13",
    "baobab": "^2.2.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "jss": "^3.2.0"
  }
}
